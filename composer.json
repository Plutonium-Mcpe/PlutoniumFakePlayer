{
    "name": "plutonium/fakeplayer-plugin",
    "description": "FakePlayer Plugin for Plutonium",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Plutonium SAS Dev Team",
            "email": "contact@plutonium.best"
        }
    ],
    "license": "MIT",
    "scripts": {
        "cs": "php vendor/bin/php-cs-fixer fix --verbose src",
        "phpstan": "php vendor/bin/phpstan analyse --memory-limit=2G src"
    },
    "autoload": {
        "psr-4": {
            "muqsit\\": "src/muqsit"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.69",
        "phpstan/phpstan": "^2.1",
        "react/promise": "^3.2",
        "plutonium/dev-phar": "^1.10",
        "plutonium/pocketmine-mp": "^5.106"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.plutonium.best"
        }
    ]
}